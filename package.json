{
  "name": "tasky",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run server:dev\" --names \"NEXT,API\" --prefix-colors \"cyan,green\"",
    "dev:frontend": "next dev",
    "dev:backend": "npm run server:dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "server:dev": "cd server && npm run dev",
    "server:setup": "cd server && npm install && npm run prisma:generate && npm run prisma:migrate",
    "setup": "npm install && npm run server:setup",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "docker:rebuild": "docker-compose up --build -d",
    "docker:studio": "docker-compose --profile studio up"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "next": "16.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "tailwindcss": "^4"
  }
}